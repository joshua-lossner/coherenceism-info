{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/roots/:path*",
      "destination": "/roots/:path*.html"
    },
    {
      "source": "/branches/:path*",
      "destination": "/branches/:path*.html"
    },
    {
      "source": "/leaves/:path*",
      "destination": "/leaves/:path*.html"
    },
    {
      "source": "/philosophy/:path*",
      "destination": "/philosophy/:path*.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}